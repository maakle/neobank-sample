  "scripts": {
    "build": "webpack --config webpack.config.js --env.BUILD=true",
    "build:analyzer": "yarn build:prod --env.ANALYZER",
    "build:demo": "NODE_ENV=production yarn build --env.PASSBASE_BACKEND='passbase-api-preprod-master-api-server.service.passbase.com' --env.FACETEC",
    "build:integrate": "yarn build --env.FACETEC --env.PASSBASE_TEST_APIKEY --env.PASSBASE_BACKEND localhost:3000 --env.NETWORK_TIMEOUT 600000 --env.PASSBASE_CDN=localhost:3000",
    "build:integrate:react": "yarn build:integrate --env.react",
    "build:npm:react": "yarn build:prod --config ./.webpack/npm.js --env.react",
    "build:npm:umd": "yarn build:prod --config ./.webpack/npm.js",
    "build:prod": "NODE_ENV=production yarn build --env.FACETEC --env.IFRAME_ROUTE='iframe_container.html'",
    "build:prod:react": "yarn build:prod --env.react",
    "build:storybook": "build-storybook -c .storybook -s ./assets -o build/storybook",
    "eslint": "eslint src/",
    "missing-translations": "npx ts-node ./scripts/getMissingTranslations.ts",
    "patch-zoom": "./scripts/patch_zoom_v8.sh",
    "postinstall": "npx rimraf node_modules/@types/react-native",
    "publish:npm": "echo $VERSION && yarn build:npm:prod && yarn publish build/npm/ --no-git-tag-version --new-version $VERSION --tag latest",
    "start": "webpack-dev-server -d --open --host localhost",
    "start-dev": "yarn start --env.PASSBASE_BACKEND_PROTOCOL='http' --env.PASSBASE_CDN_PROTOCOL='http' --env.MOCK --env.PASSBASE_TEST_APIKEY --env.PASSBASE_CDN='localhost:8080'",
    "start-dev-facetec": "yarn start-dev --env.FACETEC",
    "start-dev-facetec-https": "yarn start-dev --https --cert=./certs/passbase/cert.pem --key=./certs/passbase/key.pem --env.HTTPS --env.FACETEC",
    "start-dev-https": "yarn start-https --env.MOCK --env.PASSBASE_TEST_APIKEY --env.PASSBASE_CDN='localhost:8080'",
    "start-dev-safari": "yarn start-dev --env.SIMULATE_BROWSER Safari",
    "start-https": "yarn start --https --cert=./certs/passbase/cert.pem --key=./certs/passbase/key.pem --env.HTTPS",
    "start-integrate": "yarn start-https --env.FACETEC --env.PASSBASE_TEST_APIKEY  --env.PASSBASE_BACKEND localhost:3000 --env.NETWORK_TIMEOUT 600000 --env.PASSBASE_CDN='localhost:8080' --env.PASSBASE_BACKEND_PROTOCOL='http'",
    "start-integrate:safari": "yarn start-https --env.SIMULATE_BROWSER Safari --env.FACETEC --env.PASSBASE_TEST_APIKEY  --env.PASSBASE_BACKEND localhost:3000 --env.NETWORK_TIMEOUT 600000 --env.PASSBASE_CDN='localhost:8080'",
    "start-prod": "NODE_ENV=production yarn start-https --env.PASSBASE_BACKEND='passbase-api-preprod-master-api-server.service.passbase.com' --env.PASSBASE_TEST_APIKEY --env.FACETEC --env.PASSBASE_CDN='localhost:8080' --env.IFRAME_ROUTE='iframe_container.html'",
    "start:dev:share": "yarn start --env.MOCK --env.PASSBASE_TEST_APIKEY --env.PASSBASE_CDN",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./assets,./build/cdn",
    "storybook:dark-mode": "DARK_MODE=true yarn storybook",
    "storybook:desktop": "DESKTOP_FIRST=true yarn storybook",
    "storybook:https": "yarn storybook --https --ssl-cert certs/passbase/cert.pem --ssl-ca certs/passbase/rootCA.pem --ssl-key certs/passbase/key.pem",
    "storybook:storyshot": "STORYSHOT=YES start-storybook -p 9001 -c .storybook -s ./assets,./build/cdn",
    "test": "yarn build:storybook && yarn test:static && yarn test:unit && yarn test:structural && yarn test:e2e",
    "test-debug:e2e": "DEBUG=true yarn test:e2e",
    "test-debug:visual": "DEBUG=true yarn test:visual:light:desktop",
    "test:docker": "docker build -t passbasepuppeteer:latest -f e2e.Dockerfile . && docker run -it --cpus '.8' passbasepuppeteer:latest",
    "test:e2e": "jest -c jest.node.json --runInBand tests/e2e/",
    "test:e2e:docker": "docker build -t passbasepuppeteer:latest -f e2e.Dockerfile . && docker run -e DEMO_URL='http://localhost:8080/index.html' -it --cpus '.8' passbasepuppeteer:latest yarn test:e2e",
    "test:static": "yarn eslint",
    "test:structural": "jest -c jest.web.json tests/structural/",
    "test:unit": "jest -c jest.web.json src/",
    "test:visual:dark:desktop": "DARK_MODE=true STORYSHOT=YES PASSBASE_CDN_PROTOCOL=file yarn build:storybook && STORYSHOT=YES && DARK_MODE=true jest -c jest.web.json tests/visual_dark_desktop",
    "test:visual:dark:mobile": "DARK_MODE=true STORYSHOT=YES PASSBASE_CDN_PROTOCOL=file yarn build:storybook && STORYSHOT=YES && DARK_MODE=true jest -c jest.web.json tests/visual_dark_mobile",
    "test:visual:docker": "ts-node scripts/visual_testing/start_docker_visual_tests.ts",
    "test:visual:docker:diff": "ts-node scripts/visual_testing/run_all_visual_tests.ts",
    "test:visual:docker:update": "ts-node scripts/visual_testing/run_all_visual_tests.ts -u",
    "test:visual:light:desktop": "STORYSHOT=YES PASSBASE_CDN_PROTOCOL=file yarn build:storybook && STORYSHOT=YES jest -c jest.web.json tests/visual_light_desktop/",
    "test:visual:light:mobile": "STORYSHOT=YES PASSBASE_CDN_PROTOCOL=file yarn build:storybook && STORYSHOT=YES jest -c jest.web.json tests/visual_light_mobile"
  },
  "version": "2.0.5"
